<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="cn.chile.mapper.UserMapper">
    <select id="selectUserByUsername" parameterType="String" resultType="cn.chile.bean.User">
        SELECT * FROM users WHERE name = #{username}
    </select>
</mapper>
